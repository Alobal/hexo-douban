<blockquote>
    <p><%- quote; %></p>
</blockquote>

<style>
    <%- include('index.css',{meta_max_line:meta_max_line}) -%>
</style>

<div class="hexo-douban-tabs">
    <a class="hexo-douban-tab" id="hexo-douban-tab1" href="javascript:;"
       rel="external">
        <%= __(`${type}Do`) %>
        (<%= dO.length %>)</a>
    <a class="hexo-douban-tab" id="hexo-douban-tab2" href="javascript:;"
       rel="external">
        <%= __(`${type}Wish`) %>
        (<%= wish.length %>)</a>
    <a class="hexo-douban-tab" id="hexo-douban-tab3" href="javascript:;"
       rel="external">
        <%= __(`${type}Collect`) %>
        (<%= collect.length %>)</a>
</div>
<div>
    <div id="hexo-douban-item1">
        <% dO.forEach(function(item){ %>
            <%- include('item',{item:item}) -%>
        <% }); %>
        <%- include('pagination') -%>
    </div>
    <div id="hexo-douban-item2">
        <% wish.forEach(function(item){ %>
            <%- include('item',{item:item}) -%>
        <% }); %>
        <%- include('pagination') -%>

    </div>
    <div id="hexo-douban-item3">
        <% collect.forEach(function(item){ %>
            <%- include('item',{item:item}) -%>
        <% }); %>
        <%- include('pagination') -%>
    </div>

</div>
<%- include('script',{item_per_page:item_per_page}) -%>
